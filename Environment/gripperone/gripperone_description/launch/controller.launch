<launch>

<rosparam file="$(find gripperone_description)/launch/controller.yaml" command="load"/>
<node args="Rev45_position_controller Rev47_position_controller Rev48_position_controller Rev70_position_controller Rev72_position_controller Rev73_position_controller Rev74_position_controller joint_state_controller " name="controller_spawner" ns="gripperone" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/gripperone/joint_states"/>
</node>

</launch>
<launch>

<rosparam file="$(find gripperone_description)/launch/controller.yaml" command="load"/>
<node args="Rev75_position_controller Rev76_position_controller Rev77_position_controller Rev78_position_controller Rev79_position_controller Rev80_position_controller Rev81_position_controller joint_state_controller " name="controller_spawner" ns="gripperone" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/gripperone/joint_states"/>
</node>

</launch>